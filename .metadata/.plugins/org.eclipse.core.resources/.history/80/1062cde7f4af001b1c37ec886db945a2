package Java8;

//@FunctionalInterface
interface Addition {
	abstract int add(int a, int b);
}
class Demo implements Addition{

	//@Override
	public int add(int a, int b) {		
		return a+b;
	}
	
}
public class AddNumber {
	public static void main(String[] args) {
		Addition sum = new Demo();
		sum.add(2, 1);
	}

}
